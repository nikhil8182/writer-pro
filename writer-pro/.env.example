# Environment Variables for Writer Pro

# OpenAI API Keys (only required if using OpenAI integration)
# Get your API key from https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=""

# Default AI Model (gpt-4 or gpt-3.5-turbo)
REACT_APP_DEFAULT_AI_MODEL="gpt-4"

# Character limits for platforms
REACT_APP_TWITTER_LIMIT=280
REACT_APP_LINKEDIN_LIMIT=3000
REACT_APP_INSTAGRAM_LIMIT=2200
REACT_APP_BLOG_LIMIT=10000 